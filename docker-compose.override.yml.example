# ตัวอย่างไฟล์สำหรับ override configuration
# คัดลอกไฟล์นี้เป็น docker-compose.override.yml เพื่อแก้ไขค่าเฉพาะของคุณ

version: '3.8'

services:
  n8n:
    environment:
      # เปลี่ยน username และ password สำหรับ n8n
      - N8N_BASIC_AUTH_USER=your_username
      - N8N_BASIC_AUTH_PASSWORD=your_password
      # เปลี่ยน webhook URL ถ้าต้องการ
      - WEBHOOK_URL=http://your-domain.com/

  chat-server:
    environment:
      # เปลี่ยน webhook URL ถ้า n8n workflow ของคุณใช้ webhook path อื่น
      - N8N_WEBHOOK_URL=http://n8n:5678/webhook/your-custom-path

